#!/bin/bash

# set -x  # Output commands being run.
set -e  # Exit on error.

me=`basename "$0"`
echo "######### ${me} #############################"
NOW=$(date '+%Y%m%d')
echo "$NOW"

if [[ "$1" = listen ]]; then
   sudo lsof -i -P -n | grep LISTEN
      

      
exit 0         
else
   1>&2 echo "Invalid arguments. Usage:

./${me} size | topfile | rename from to"
fi
