
ALPHA   : /[a-zA-z]/
BIT     : "0" | "1"
CHAR    : /[\x00-\x7f]/
CR      : "\r"
CRLF    : CR LF
CTL     : /[\x00-\x1f]/ | "\x7f"
DIGIT   : /[0-9]/
DQUOTE  : "\""
HEXDIG  : DIGIT | "A"i | "B"i | "C"i | "D"i | "E"i | "F"i
HTAB    : "\x09"
LF      : "\n"
LWSP    : (WSP | CRLF WSP)*
OCTET   : /[\x00-\xFF]/
SP      : "\x20"
VCHAR   : "[\x21-\x7e]"
WSP     : SP | HTAB